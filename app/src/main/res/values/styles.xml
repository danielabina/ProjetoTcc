<resources>

    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.DarkActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">#1b1464</item>
        <item name="colorPrimaryDark">#1b1464</item>
        <item name="colorAccent">@color/colorAccent</item>
    </style>

    <style name="EditTextFormField"></style>

    <style name="ButtonForm"></style>

    <style name="TextViewLink"></style>

    <style name="NoActionBar" parent="Theme.AppCompat.Light.NoActionBar">
        <item name="windowActionBar">false</item>
        <item name="colorPrimary">#1b1464</item>
        <item name="windowNoTitle">true</item>
    </style>

    <style name="AppBarOverlay" parent="ThemeOverlay.AppCompat.Dark.ActionBar">
        <item name="colorPrimary">#1b1464</item>
    </style>


    <style name="PopupOverlay" parent="@style/Theme.AppCompat.DayNight">
        <item name="colorPrimary">#1b1464</item>
    </style>

    <style name="AppTheme.AppBarOverlay" parent="ThemeOverlay.AppCompat.Dark.ActionBar" />

    <style name="AppTheme.PopupOverlay" parent="ThemeOverlay.AppCompat.Light" />



</resources>
