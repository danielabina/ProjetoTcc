<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical"
    android:layout_height="match_parent"
    android:layout_width="match_parent"
   >
    />

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        android:layout_marginTop="100dp"
        android:id="@+id/IdfragmentAvaliacao">


        <com.google.android.material.textfield.TextInputLayout
            android:id="@+id/til_card_number"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginLeft="30dp"
            android:layout_weight="1.2">

            <TextView
                android:id="@+id/nome"
                android:layout_width="match_parent"
                android:layout_height="30dp"
                android:textSize="20dp"
                android:layout_marginLeft="20dp"
                android:text="Otimo Jogador,porem deve aprimorar seu ataque" />

        </com.google.android.material.textfield.TextInputLayout>

    </LinearLayout>


    <RatingBar
        android:id="@+id/ratingBar"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="80dp"
        android:numStars="5"
        android:rating="2.0"
        android:stepSize="1.0" />

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        android:layout_marginTop="100dp">



        <com.google.android.material.textfield.TextInputLayout
            android:id="@+id/til_card_number3"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginLeft="30dp"
            android:layout_weight="1.2">

            <TextView
                android:id="@+id/nome2"
                android:layout_width="match_parent"
                android:layout_height="30dp"
                android:textSize="20dp"
                android:layout_marginLeft="20dp"
                android:text="Bom,vencemos de 4 x 0,fez 3 pontos" />

        </com.google.android.material.textfield.TextInputLayout>

    </LinearLayout>


    <RatingBar
        android:id="@+id/ratingBar1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="80dp"
        android:numStars="5"
        android:rating="2.0"
        android:stepSize="1.0" />

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        android:layout_marginTop="100dp">



        <com.google.android.material.textfield.TextInputLayout
            android:id="@+id/til_card_number5"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginLeft="30dp"
            android:layout_weight="1.2">

            <TextView
                android:id="@+id/nome3"
                android:layout_width="match_parent"
                android:layout_height="30dp"
                android:textSize="20dp"
                android:layout_marginLeft="10dp"
                android:text="Regular, deve trabalhar mais em equipe" />

        </com.google.android.material.textfield.TextInputLayout>

    </LinearLayout>


    <RatingBar
        android:id="@+id/ratingBar5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="80dp"
        android:numStars="5"
        android:rating="2.0"
        android:stepSize="1.0" />


</LinearLayout>